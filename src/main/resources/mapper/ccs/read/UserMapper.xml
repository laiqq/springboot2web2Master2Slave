<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jony.platform.mapper.ccs.read.UserMapper" >

	<select id="selectUser"  resultType="com.jony.platform.model.entity.User">
  	SELECT * 
  	FROM t_user 
  	WHERE user_name=#{userName}
  	
  	</select>
  	
	
</mapper>